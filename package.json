{
  "name": "getty",
  "version": "1.1.1",
  "type": "commonjs",
  "packageManager": "pnpm@9.12.3",
  "description": "Complete and customizable app with tools for live streaming. Tip notifications, tip goals, chat widget, and real-time alerts for your live streams on Odysee.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "npm-run-all copy:primeicons copy:landing --parallel dev-server dev-css",
    "dev-server": "nodemon --config nodemon.json",
    "admin:dev": "vite --config admin-frontend/vite.config.js",
    "admin:build": "vite build --config admin-frontend/vite.config.js",
    "dev-css": "tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit",
    "lint:css": "stylelint \"src/**/*.css\" --cache",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix --cache",
    "build-css": "tailwindcss -i src/input.css -o public/css/output.css",
    "watch-css": "tailwindcss -i src/input.css -o public/css/output.css --watch",
    "minify-html": "html-minifier-terser src/landing.html -o public/index.html --collapse-whitespace --remove-comments --minify-css false",
    "minify-js": "esbuild public/js/*.js --minify --outdir=public/js/min",
    "minify-css": "postcss public/css/output.css -o public/css/output.css --config postcss.config.cjs",
    "build:i18n": "node scripts/build-i18n.js",
    "build-dashboard": "esbuild src/index.js --bundle --format=cjs --outfile=public/js/dashboard.js",
    "build": "npm-run-all --sequential copy:primeicons copy:landing build-css minify-css build:i18n build-dashboard admin:build minify-html minify-js add-sri",
    "build:render": "cross-env BUILD_LIGHT=1 npm-run-all --sequential copy:primeicons copy:landing build-css minify-css build:i18n minify-html minify-js add-sri",
    "add-sri": "node scripts/add-sri.js",
    "copy:primeicons": "node scripts/copy-primeicons.js",
    "copy:landing": "node scripts/copy-landing-assets.js",
    "lint": "eslint admin-frontend/src modules routes scripts server.js --ext .js,.vue --cache --max-warnings=0",
    "lint:fix": "eslint admin-frontend/src modules routes scripts server.js --ext .js,.vue --fix --cache",
    "report:unused-css": "node scripts/unused-css-report.js",
    "verify:hosted": "node scripts/hosted-verify.js"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "dependencies": {
    "@primevue/themes": "^4.3.9",
    "@supabase/supabase-js": "^2.58.0",
    "@untitledui/icons": "^0.0.19",
    "@wanderapp/connect": "^0.1.12",
    "arweave": "^1.15.7",
    "axios": "^1.12.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "image-size": "^2.0.2",
    "ioredis": "^5.8.0",
    "lucide-vue-next": "^0.544.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "obs-websocket-js": "^5.0.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.1",
    "react-aria-components": "^1.12.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-react-aria-components": "^1.0.0",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.3.0",
    "ws": "^8.13.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@swc/core": "^1.6.0",
    "@swc/jest": "^0.2.36",
    "@tailwindcss/forms": "^0.5.10",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/eslint-config-standard": "^9.0.1",
    "@vue/test-utils": "^2.4.6",
    "arconnect": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^10.0.0",
    "cssnano": "^7.1.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "fast-glob": "^3.3.2",
    "html-minifier-terser": "^7.2.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.2.0",
    "micromatch": "^4.0.7",
    "nock": "^14.0.10",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "purgecss": "^7.0.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.4.20",
    "vue": "^3.5.22"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.25.9"
    }
  }
}
