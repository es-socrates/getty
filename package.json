{
  "name": "getty",
  "version": "1.0.0",
  "description": "App widget for Odysee streamers to manage tips, goals, and chat.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js & npx tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "npx tailwindcss -i src/styles.css -o public/css/output.css",
    "watch-css": "npx tailwindcss -i src/styles.css -o public/css/output.css --watch",
    "build": "npx tailwindcss -i ./src/input.css -o ./public/css/output.css --minify"
  },
  "dependencies": {
    "@untitledui/icons": "^0.0.19",
    "arweave": "^1.15.7",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "react-aria-components": "^1.11.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-react-aria-components": "^1.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "stylelint": "^16.21.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
